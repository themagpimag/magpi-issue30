        <PAGEOBJECT OwnPage="2" PTYPE="4" XPOS="128.34708661126" YPOS="1838.39503937032" WIDTH="538.5883465" HEIGHT="710.739448818658" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="pale python green" PCOLOR2="None" COLUMNS="1" COLGAP="25.51181102" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LINESP="10" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" IFONT="Monaco Regular" ISIZE="8" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="11.3385826771654" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="0" LOCKR="0" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 538.588 0 538.588 0 538.588 0 538.588 0 538.588 710.739 538.588 710.739 538.588 710.739 538.588 710.739 0 710.739 0 710.739 0 710.739 0 710.739 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 538.588 0 538.588 0 538.588 0 538.588 0 538.588 710.739 538.588 710.739 538.588 710.739 538.588 710.739 0 710.739 0 710.739 0 710.739 0 710.739 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="6" BOOKMARK="0" NEXTITEM="42" BACKITEM="-1">
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="#!/usr/bin/env python"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="import random"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="def build_maze(maze):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  deep = len(maze)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  wide = len(maze[0])"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  print &quot;Am building a&quot;,wide,&quot;by&quot;,deep,&quot;maze&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  "/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  x = 0"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  y = 0"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  maze[y][x]= 4"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  total = wide*deep"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  blocks_tagged = 1"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  while (blocks_tagged &lt; total):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    options=[]"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    if (x &gt; 0):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (maze[y][x-1] == 0):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        options.append(&quot;left&quot;)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    if (x &lt; wide-1):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (maze[y][x+1] == 0):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        options.append(&quot;right&quot;)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    if (y &gt; 0):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (maze[y-1][x] == 0):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        options.append(&quot;up&quot;)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    if (y &lt; deep-1):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (maze[y+1][x] == 0):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        options.append(&quot;down&quot;)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        "/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    if (options):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      choice_index = random.randrange(len(options))"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      choice = options[choice_index]"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      blocks_tagged +=1"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      "/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (choice == 'down'):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        if (maze[y][x] == 3):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="          maze[y][x] =1         "/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        if (maze[y][x] == 4):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="          maze[y][x] =2"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        y = y + 1"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        maze[y][x] = 4"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (choice == 'up'):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        y = y -1"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        maze[y][x]=2"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (choice == 'right'):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        if (maze[y][x] == 4):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="          maze[y][x] = 3"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        if (maze[y][x] == 2):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="          maze[y][x] = 1"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        x = x+1"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        maze[y][x] = 4"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (choice == 'left'):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        x = x -1"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        maze[y][x] = 3       "/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    else:"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      while(1):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        y = random.randrange(deep)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        x = random.randrange(wide)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        if (maze[y][x] &gt; 0):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="          break"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="def print_asci_maze(maze):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  deep = len(maze)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  wide = len(maze[0])"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  right_wall = (0,2,4)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  bottom_wall = (0,3, 4)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    "/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  # print top border"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  row = &quot;+s+&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  for index in range (0,wide-1):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    row +=&quot;-+&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  print row"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  for index1 in range (0,deep):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    # print upper half of row"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    row = &quot;|&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    for index in range (0,wide):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (maze[index1][index] in right_wall):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        row += &quot; |&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      else:"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        row += &quot;  &quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    print row"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    # print lower half of row"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    row = &quot;+&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    for index in range (0,wide):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      if (index == wide-1) and (index1==deep-1):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        row +=&quot;f+&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="      else:"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        if (maze[index1][index] in bottom_wall):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="          row +=&quot;-+&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="        else:"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="          row += &quot; +&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    print row"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT CH="# The main function"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="def main():"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  print  &quot;&quot;&quot;\n\nMaze Builder. Inspired By"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  &quot;The Creative Atari&quot; 1983\n\n&quot;&quot;&quot;"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  width = 0"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  depth = 0"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  while (width &lt; 2) or (width &gt; 10):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    width = int(raw_input(&quot;What should the width be (2-10)?&quot;))"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  while (depth &lt; 2) or (depth &gt; 10):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    depth = int(raw_input(&quot;What should the depth be (2-10)?&quot;))"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  print  &quot;Your maze will be&quot;,width,&quot;by&quot;,depth"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  # set up a list of lists referenced as maze[row][column]"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  maze = [0 for index in range (0, depth)]"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  for index in range (0, depth):"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="    maze[index] = [0]*width"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  build_maze(maze)"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  print_asci_maze(maze)"/>
            <para PARENT="Code" LINESP="10"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="if __name__ == '__main__':"/>
            <para PARENT="Code" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="8" CH="  main()"/>
            <para PARENT="Code" LINESP="10"/>
