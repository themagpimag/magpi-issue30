            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH=" TwoVector MatrixTransform::transform(const TwoVector &amp;twoVector){"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   double x = twoVector.x();"/>
            <ITEXT CH="  // Get the x-component of the 2d vector"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   double y = twoVector.y();"/>
            <ITEXT CH="  // Get the y-component of the 2d vector"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   double new_x = m_normalisation * (x*m_matrix[0][0] + y*m_matrix[0][1]);"/>
            <ITEXT CH="  // matrix mult."/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   double new_y = m_normalisation * (x*m_matrix[1][0] + y*m_matrix[1][1]);"/>
            <ITEXT CH="  // matrix mult."/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   return TwoVector(new_x,new_y);"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH=" }"/>
            <para PARENT="Code"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH=" std::string MatrixTransform::str(){"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   std::stringstream ss;"/>
            <ITEXT CH="  // Create a string stream instance"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   ss &lt;&lt; std::setw(4) &lt;&lt; m_normalisation &lt;&lt; &quot; x &quot;;"/>
            <ITEXT CH="  // Print the normalisation and matrix"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   ss &lt;&lt; &quot;| &quot; &lt;&lt; std::setw(3) &lt;&lt; m_matrix[0][0] &lt;&lt; &quot;, &quot; &lt;&lt; std::setw(3) &lt;&lt; m_matrix[0][1]"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="      &lt;&lt; &quot; |&quot; &lt;&lt; std::endl;"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   ss &lt;&lt; &quot;       | &quot; &lt;&lt; std::setw(3) &lt;&lt; m_matrix[1][0] &lt;&lt; &quot;, &quot; &lt;&lt; std::setw(3) &lt;&lt; m_matrix[1][1] "/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="      &lt;&lt; &quot; |&quot; &lt;&lt; std::endl;"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH="   return ss.str();"/>
            <ITEXT CH="  // Return the string value from the string stream instance"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH=" }"/>
